  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "unplugin-icons/types/vue"
    ],
    "paths": {
      "#imports": ["./.nuxt/imports"],
      "#app": ["./.nuxt/app"],
      "#components": ["./.nuxt/components"],
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  }
} 